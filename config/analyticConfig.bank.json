  "analysisFunctions": [
    {
      "id": "avg",
      "spdzPgm": "analytics_avg",
      "name": "Average",
      "description": "Calculate avg as \u03A3 localSum / \u03A3 localCount.",
      "example":
        "select sum(col_1), count(col_1) \n  from table_name \n where col2 >1000",
      "inputs": [
        {
          "name": "localSum",
          "type": "int",
          "byteSize": 4
        },
        {
          "name": "localCount",
          "type": "int",
          "byteSize": 4
        }
      ],
      "inputRowCount": {
        "rowBufferSize": 1,
        "batched": false
      },
      "outputs": [
        {
          "name": "result",
          "type": "float",
          "byteSize": 16
        }
      ],
      "outputRowCount": 1
    },
    {
      "id": "percentHour",
      "spdzPgm": "distrib_percent",
      "name": "Percentage by hour",
      "description": "Percentage split of a summed value across hour.",
      "example":
        "select hour(incidentDate), count(*) \n  from table_name \n group by hour(incidentDate)",
      "inputs": [
        {
          "name": "xIndex",
          "type": "int",
          "byteSize": 4
        },
        {
          "name": "yCount",
          "type": "int",
          "byteSize": 4
        }
      ],
      "inputRowCount": {
        "rowBufferSize": 24,
        "batched": false
      },
      "outputs": [
        {
          "name": "xIndex",
          "type": "float",
          "byteSize": 16
        },
        {
          "name": "yPercent",
          "type": "float",
          "byteSize": 16
        }
      ],
      "outputRowCount": 24
    },
    {
      "id": "sumlookup",
      "spdzPgm": "sumby_lookup",
      "name": "Sum by lookup",
      "description":
        "Sum of quantity grouped across lookup value. Party 1 supplies quantity.",
      "example":
        "Party 1\nselect identifier, quantity\n  from table_name\n \nParty 2\nselect identifier, lookupvalue\n  from table_name",
      "inputs": [
        {
          "name": "identifier",
          "type": "int",
          "byteSize": 4
        },
        {
          "name": "yQuantity",
          "type": "int",
          "byteSize": 4
        }
      ],
      "inputRowCount": {
        "rowBufferSize": 50,
        "batched": true
      },
      "outputs": [
        {
          "name": "xLookupValue",
          "type": "float",
          "byteSize": 16
        },
        {
          "name": "y%Quantity",
          "type": "float",
          "byteSize": 16
        }
      ],
      "outputRowCount": 31
    }
  ]
